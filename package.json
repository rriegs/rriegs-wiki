{
  "name": "rriegs-wiki",
  "version": "0.1.0",
  "description": "A single-page, small-content wiki implemented in Elm.",
  "main": "index.js",
  "repository": "git@github.com:rriegs/rriegs-wiki.git",
  "author": "Ryan Riegel <rriegs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "micro",
    "dev": "micro-dev -w index.js & npm run watch",
    "build": "elm-make src/Main.elm --output www/main.html",
    "watch": "chokidar 'src/**/*.elm' -c 'npm run build' --initial"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "micro": "^9.1.0",
    "microrouter": "^3.0.0",
    "mime": "^2.2.0"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "elm": "^0.18.0",
    "micro-dev": "^2.2.0"
  }
}
